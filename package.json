{
  "name": "mdimage",
  "version": "0.0.1",
  "description": "markdown image parser(parse to base64)",
  "main": "./lib/cli.cjs",
  "module": "./lib/marked.esm.js",
  "type": "module",
  "browser": "./lib/marked.umd.js",
  "author": "fengyon",
  "bin": {
    "mdimage": "bin/index.cjs"
  },
  "exports": {
    ".": {
      "import": "./lib/marked.esm.js",
      "default": "./lib/marked.cjs"
    },
    "./bin/marked": "./bin/marked.js",
    "./package.json": "./package.json"
  },
  "repository": "git@github.com:fengyon/mdimage.git",
  "homepage": "http://github.com/fengyon/mdimage",
  "bugs": {
    "url": "http://github.com/fengyon/mdimage/issues"
  },
  "files": [
    "bin/",
    "lib/",
    "src/",
    "index.min.js"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "test": "echo 'ok'",
    "changelogs": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "prettier src/**/*.{js,ts,json} --write && eslint src/**/*.{js,ts} --fix",
    "commit": "cz",
    "dev": "rollup -w -c rollup.config.js --environment FORMAT:esm",
    "build": "rimraf lib/* && rollup -c rollup.config.js --bundleConfigAsCjs",
    "start": "npm run build && node bin/index.cjs"
  },
  "keywords": [
    "markdown",
    "image",
    "parser"
  ],
  "tags": [
    "markdown",
    "image",
    "parser"
  ],
  "pre-commit": [
    "eslint"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint",
      "pre-commit": "lint-staged",
      "pre-push": "test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.7",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/marked": "^4.0.8",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.16",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel": "^6.23.0",
    "chai": "^4.3.7",
    "commitizen": "^4.2.6",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.1",
    "rollup": "^3.7.5",
    "rollup-plugin-node-externals": "^5.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "chalk": "4.1.0",
    "commander": "^9.4.1",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.4",
    "marked": "^4.2.4",
    "ora": "^6.1.2",
    "tslib": "^2.5.0"
  }
}
